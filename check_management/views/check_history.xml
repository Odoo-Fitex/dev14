<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

<!--        payment check Line tree view&ndash;&gt;-->
        <record model="ir.ui.view" id="payment_check_line__history_tree">
            <field name="name">History Check lines</field>
            <field name="model">check.history</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="check_number"/>
                    <field name="check_date"/>
                    <field name="check_amount"/>
                    <field name="reason"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>